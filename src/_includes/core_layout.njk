<input type="radio" name="answer-{{ item_num }}" id="input-answer-{{ item_num }}" class="counter_ignore input-answer" />
<input type="radio" name="question-{{ item_num }}" id="question-{{ item_num }}-correct" class="correct-answer input-answer" />
<input type="radio" name="question-{{ item_num }}" id="question-{{ item_num }}-incorrect" class="counter_ignore incorrect-answer input-answer" />
<section id="question-{{ item_num }}" page="q{{ item_num }}">
    <article>
        {% block content %}
        {% endblock %}
    </article>
    <picture>
        {# <lottie-player src="https://assets3.lottiefiles.com/packages/lf20_jqtrvhhf.json"  background="transparent"  speed="1"  style="width: 300px; height: 300px;"  loop autoplay> #}
        <img src="https://fs.hubspotusercontent00.net/hubfs/2661559/Teneo/Work%20from%20Anywhere%20-%20Lie%20Detector/placeholder_image.png" class="placeholder" />
    </picture>
</section>
{% if question.answer %}
<section id="answer-{{ item_num }}" page="a{{ item_num }}">
    <article>
        <h3 class="answer">{{ question.answer.header | safe }}</h3>
        <p class="explanation">{{ question.answer.copy | safe }}</p>
        <div class="cta_container">
            <label for="input-answer-{{ item_num }}" class="cta">Next</label>
            <a href="#" target="_blank">
                <span>Learn more</span>
                <svg width="31" height="39" viewBox="0 0 31 39" fill="none" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink">
                    <rect width="31" height="39" fill="url(#pattern0)"/>
                    <defs>
                    <pattern id="pattern0" patternContentUnits="objectBoundingBox" width="1" height="1">
                    <use xlink:href="#image0_92_6" transform="translate(-0.129032) scale(0.00629032 0.005)"/>
                    </pattern>
                    <image id="image0_92_6" width="200" height="200" xlink:href="data:image/png;base64,"/>
                    </defs>
                    </svg>

            </a>
        </div>
    </article>
</section>
{% endif %}